<h1>Login</h1>
<p>Please login to your Pimped account</p>

<%= form_tag sessions_path do |f| %>

  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div><br>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div><br>
  
  <div class="actions">
    <%= submit_tag "login" %>
  </div>

<% end %>
  